[run]
source =
    urllib3

[paths]
source =
    src/urllib3
    */urllib3
    *\urllib3

[report]
omit =
    src/urllib3/packages/*
    src/urllib3/contrib/appengine.py
    src/urllib3/contrib/ntlmpool.py
    src/urllib3/contrib/pyopenssl.py
    src/urllib3/contrib/securetransport.py
    src/urllib3/contrib/_securetransport/*

exclude_lines =
    except ModuleNotFoundError:
    except ImportError:
    pass
    import
    raise
    .* # Platform-specific.*
    .*:.* # Python \d.*
    .* # Abstract
    .* # Defensive:
